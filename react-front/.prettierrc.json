{
  "bracketSpacing": true,
  "printWidth": 140,
  "singleQuote": false,
  "semi": true,
  "trailingComma": "none",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "auto",
  "quoteProps": "consistent",
  "arrowParens": "always",
  "insertPragma": false,
  "importExtensions": [".js", ".jsx"],
  "plugins": ["prettier-plugin-tailwindcss"]
}
